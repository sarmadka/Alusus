import "defs-ignore.alusus";
def print: @expname[printf] function (fmt: ptr[Word[8]], args: ...any)=>Int[64];

class A {
    def i: Int;
    handler this~init() {
        print("A~init\n");
        this.i = 6;
    }
    handler this~terminate() {
        print("A~terminate\n");
    }
}

def a: A;

@expname[main] func test {
    print("%d\n", a.i);
}

Spp.buildMgr.dumpLlvmIrForElement(test~ast);

// Test the order of global variable initializations.

def global1: Int(3);
def global2: Int(global1 + 1);

func testGlobalVarInitOrder () {
  global1~ptr;
  depFunc();
}

func depFunc {
  global2~ptr;
}

Spp.buildMgr.dumpLlvmIrForElement(testGlobalVarInitOrder~ast);
